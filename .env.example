# Web Search Configuration (optional - will use mock data if not provided)
SEARCH_PROVIDER=duckduckgo
SERPER_API_KEY=your_serper_api_key_here
SEARCH_ENGINE_ID=your-search-engine-id-here

# Demo Configuration
DEMO_MODE=development
LOG_LEVEL=INFO
